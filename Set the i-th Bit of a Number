// Online C compiler to run C program online
#include <stdio.h>
// Set the i-th Bit of a Number
// return new result 
// 1010 
// 0001
// 1011

int setBit(int n, int i) {
    int mask = 1 << i;
    printf(" %d ",mask);
    return n | mask;
}
int main() {
    // Write C code here
    int result = setBit(10,0);
    printf("%d", result);

    return 0;
}
